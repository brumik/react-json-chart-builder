{
  "name": "@ansible/react-json-chart-builder",
  "version": "2.0.0-semantic",
  "private": false,
  "description": "A chart builder component in react, which uses JSON for schema.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ansible/react-json-chart-builder.git"
  },
  "keywords": [
    "react",
    "chart",
    "json",
    "builder",
    "schema"
  ],
  "author": "Levente Berky",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:demo:docs": "typedoc --out demo/dist/docs --name 'React Json Chart Builder' src/index.ts",
    "build:demo:app": "webpack --config demo/webpack.config.js --mode production",
    "build:demo": "npm run build:demo:app && npm run build:demo:docs",
    "start": "webpack serve --config demo/webpack.config.js --mode development",
    "test": "jest --config jest.config.json --coverage",
    "test:watch": "jest --config jest.config.json --watch",
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc -p tsconfig.esm.json --module es2015 --target es5",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "lint": "eslint ./src/**/*.{js,ts,tsx} ./demo/src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{js,ts,tsx} ./demo/src/**/*.{js,ts,tsx} --fix",
    "prepublishOnly": "npm run lint",
    "prepare": "rm -rf dist && npm run build"
  },
  "peerDependencies": {
    "@patternfly/react-charts": "^7.0.1",
    "@patternfly/react-core": "^5.0.0",
    "react": "^17.0.2 | ^18",
    "react-dom": "^17.0.2 | ^18"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@patternfly/react-charts": "7.1.1",
    "@patternfly/react-code-editor": "^5.1.1",
    "@patternfly/react-core": "^5.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.5.2",
    "@types/react": "^17.0.69",
    "@types/react-dom": "^17.0.22",
    "@types/react-redux": "^7.1.28",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.29",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "react": "*",
    "react-dom": "*",
    "react-router-dom": "^6.17.0",
    "semantic-release": "^22.0.5",
    "style-loader": "^3.3.3",
    "styled-components": "^6.1.0",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.5.0",
    "typedoc": "^0.25.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "uuid": "^9.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
