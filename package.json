{
  "name": "react-json-chart-builder",
  "version": "0.0.0-semantic",
  "private": false,
  "description": "A chart builder component in react, which uses JSON for schema.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/brumik/react-json-chart-builder.git"
  },
  "keywords": [
    "react",
    "chart",
    "json",
    "builder",
    "schema"
  ],
  "author": "Levente Berky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brumik/react-json-chart-builder/issues"
  },
  "homepage": "https://brumik.github.io/react-json-chart-builder",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:demo:docs": "typedoc --out demo/dist/docs --name 'React Json Chart Builder' src/index.ts",
    "build:demo:app": "webpack --config demo/webpack.config.js --mode production",
    "build:demo": "npm run build:demo:app && npm run build:demo:docs",
    "start": "webpack serve --config demo/webpack.config.js --mode development",
    "test": "jest --config jest.config.json --coverage",
    "test:watch": "jest --config jest.config.json --watch",
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc -p tsconfig.esm.json --module es2015 --target es5",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "lint": "eslint ./src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{js,ts,tsx} --fix",
    "prepublishOnly": "npm run lint",
    "prepare": "rm -rf dist && npm run build"
  },
  "peerDependencies": {
    "@patternfly/react-charts": "^6.28.1",
    "@patternfly/react-core": "^4.175.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@patternfly/react-charts": "^6.28.1",
    "@patternfly/react-code-editor": "^4.2.92",
    "@patternfly/react-core": "^4.175.10",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.16",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^35.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "react": "*",
    "react-dom": "*",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
